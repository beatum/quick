-# Javascript
-load static
-if settings.DEBUG
  %script{src: "{% static 'jquery/dist/jquery.js' %}", type: "text/javascript"}
  %script{src: "{% static 'bootstrap/dist/js/bootstrap.js' %}", type: "text/javascript"}
  %script{src: "{% static 'bootstrap-material-design/dist/js/material.js' %}", type: "text/javascript"}
  %script{src: "{% static 'bootstrap-material-design/dist/js/ripples.js' %}", type: "text/javascript"}
  %script{src: "{% static 'underscore/underscore.js' %}", type: "text/javascript"}
  %script{src: "{% static 'angular/angular.js' %}", type: "text/javascript"}
