  %script{src: "{% static 'angular-cookies/angular-cookies.min.js' %}", type: "text/javascript"}
  %script{src: "{% static 'ngDialog/js/ngDialog.min.js' %}", type: "text/javascript"}
  %script{src: "{% static 'snackbarjs/dist/snackbar.min.js' %}", type: "text/javascript"}

